{
  "definitions": [
    {
      "title": "Product Landing Template",
      "id": "product-landing-template",
      "plugins": {
        "xwalk": {
          "page": {
            "resourceType": "core/franklin/components/template/v1/template",
            "template": {
              "name": "Product Landing",
              "model": "product-landing-model",
              "blocks": [
                {
                  "type": "hero-banner",
                  "config": {
                    "headline": "Welcome to Our Product",
                    "backgroundImage": "/images/hero.jpg"
                  }
                },
                {
                  "type": "feature-list",
                  "config": {
                    "features": ["Fast", "Reliable", "Secure"]
                  }
                },
                {
                  "type": "testimonial",
                  "config": {
                    "quote": "This product changed my life!",
                    "author": "Happy Customer"
                  }
                }
              ]
            }
          }
        }
      }
    }
  ],
  "models": [
    {
      "id": "product-landing-model",
      "fields": [
        {
          "component": "reference",
          "name": "blocks",
          "label": "Blocks",
          "valueType": "array",
          "multi": true
        }
      ]
    }
  ],
  "filters": [
    {
      "id": "page",
      "components": ["product-landing-template"]
    }
  ]
}
